{
    "name": "express-ts-boilerplate",
    "version": "0.0.1",
    "description": "My express TypeScript boilerplate",
    "author": "Alvian D.Q. (https://github.com/Alviannn)",
    "license": "MIT",
    "scripts": {
        "dev": "cross-env NODE_ENV=development ts-node ./src/server.ts",
        "start": "node ./build/server.js",
        "seed": "cross-env NODE_ENV=development ts-node ./scripts/seeder.ts",
        "clean": "rimraf ./build",
        "typeorm": "typeorm-ts-node-commonjs",
        "jwt:generate": "ts-node ./scripts/jwt-generate.ts",
        "compile": "yarn clean && tsc",
        "compile:debug": "yarn compile --diagnostics",
        "lint": "eslint ./src",
        "lint:fix": "eslint ./src --fix",
        "migration:generate": "cross-env NODE_ENV=development ts-node ./scripts/migrations.ts GENERATE",
        "migration:create": "cross-env NODE_ENV=development ts-node ./scripts/migrations.ts CREATE",
        "migration:run": "cross-env NODE_ENV=development ts-node ./scripts/migrations.ts RUN",
        "migration:revert": "cross-env NODE_ENV=development ts-node ./scripts/migrations.ts REVERT",
        "migration:show": "cross-env NODE_ENV=development ts-node ./scripts/migrations.ts SHOW"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/compression": "^1.7.2",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/jsonwebtoken": "^8.5.8",
        "@types/luxon": "^2.3.2",
        "@types/morgan": "^1.9.3",
        "@types/node": "^17.0.23",
        "@types/pg": "^8.6.5",
        "@typescript-eslint/eslint-plugin": "^5.21.0",
        "@typescript-eslint/parser": "^5.22.0",
        "cross-env": "^7.0.3",
        "eslint": "^8.14.0",
        "eslint-import-resolver-typescript": "^2.7.1",
        "eslint-plugin-import": "^2.26.0",
        "rimraf": "^3.0.2",
        "ts-node": "^10.7.0",
        "typescript": "^4.6.3"
    },
    "dependencies": {
        "bcrypt": "^5.0.1",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "dotenv": "^16.0.0",
        "express": "^4.18.1",
        "helmet": "^5.0.2",
        "http-status-codes": "^2.2.0",
        "joi": "^17.6.0",
        "jsonwebtoken": "^8.5.1",
        "luxon": "^2.3.2",
        "morgan": "^1.10.0",
        "pg": "^8.7.3",
        "reflect-metadata": "^0.1.13",
        "typeorm": "^0.3.6",
        "winston": "^3.7.2",
        "winston-daily-rotate-file": "^4.6.1"
    }
}
